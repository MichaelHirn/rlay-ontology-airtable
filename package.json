{
  "name": "rlay-ontology-airtable",
  "version": "1.0.2",
  "description": "Rlay ontology schema for airtable integrations",
  "main": "src/index.js",
  "scripts": {
    "seed": "rlay-seed --backend myneo4j --input src/ontology.js > src/generated/cids.json",

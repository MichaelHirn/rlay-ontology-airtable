  "dependencies": {
    "@rlay/rlay-client-lib": "^0.2.0",
    "@rlay/transform": "^0.3.0",
    "@rlay/web3-rlay": "^0.1.14",
    "airtable": "^0.7.1"
  },
  "devDependencies": {
    "@rlay/utils": "0.0.5",
    "mocha": "^6.2.0",
    "simple-mock": "^0.8.0",
    "sinon": "^7.5.0"
  }
}
